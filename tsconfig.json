{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "allowJs": true,
        "moduleResolution": "node",
        "module": "esnext",
        "target": "es2015",
        "jsx": "react"
    },
    "lib": [
        "es2015"
    ],
    "include": [
        "./src/*"
    ],
    "exclude": [
        "./node_modules",
        "./build",
        "./webpack",
        "./document"
    ]
}